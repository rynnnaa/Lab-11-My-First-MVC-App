@model List<Lab_11_My_First_MVC_App.Models.TimePerson>


<DOCTYPE html>
    <html>
    <head>
        <title>Results</title>
        <link href="~/StyleSheet.css" rel="stylesheet" />
    </head>
    <body>
        <h1>HERE ARE THE RESULTS</h1>

        <table>
            <thread>
                <tr>
                    <th scope="col">Year</th>
                    <th scope="col">Honor</th>
                    <th scope="col">Name</th>
                    <th scope="col">Country</th>
                    <th scope="col">BirthYear</th>
                    <th scope="col">DeathYear</th>
                    <th scope="col">Title</th>
                    <th scope="col">Category</th>
                    <th scope="col">Context</th>
                    <th scope="col">
                </tr>
            </thread>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Year</td>
                    <td>@item.Honor</td>
                    <td>@item.Name</td>
                    <td>@item.Country</td>
                    <td>@item.BirthYear</td>
                    <td>@item.DeathYear</td>
                    <td>@item.Title</td>
                    <td>@item.Category</td>
                    <td>@item.Context</td>
                </tr>
            }


        </table>

    </body>
</html>
</DOCTYPE>